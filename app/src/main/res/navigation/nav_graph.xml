        android:id="@+id/fuelTypeFragment"
        android:name="martialcoder.surajsahani.turbocaretestassignment.Fragments.FuelTypeFragment"
        android:label="fragment_fuel_type"
        tools:layout="@layout/fragment_fuel_type" >
        <action
            android:id="@+id/action_fuelTypeFragment_to_vehicleProfileFragment"
            app:destination="@id/vehicleProfileFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit"
            app:popUpTo="@id/vehicleProfileFragment"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
</navigation>